package excep;

public class excep {

	public static void main(String[] args) {
		int a[] = {5,10};
		int b=5;
		try {
			int x=a[2]/b-a[1];
		}
		catch(NumberFormatException e) {
			System.out.println(e);
		}
		catch(ArithmeticException e) {
			System.out.println("Devision by 0");
		}
		catch(Exception e) {
			System.out.println("Exception: " + e);
		}
		int y = a[1]/a[0];
		System.out.println("y = " + y);

	}

}
