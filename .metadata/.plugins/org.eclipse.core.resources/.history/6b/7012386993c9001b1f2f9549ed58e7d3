
public class ArrayExcept {

	public static void main(String[] args) {
		int[] array = new int[5];
		
		array[0] = 5684;
		array[1] = 7563;
		array[2] = 234634;
		array[3] = 95678;
		array[4] = 123;
		
		try {
			System.out.println(array[5]);
		}
		catch(ArrayIndexOutOfBoundsException e) {
			System.out.println(e);
		}
	}

}
